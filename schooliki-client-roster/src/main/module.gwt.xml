<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="teacher">
	<inherits name="com.google.gwt.query.Query" />
	<inherits name="gwtquery.plugins.Ui" />
	<inherits name="com.google.common.collect.Collect" />
	<inherits name="com.floreysoft.gwt.picker.Picker" />
	<inherits name='com.google.gwt.place.Place' />
	<inherits name='com.google.gwt.activity.Activity' />
	<inherits name="gwt.material.design.GwtMaterialDesignBasic" />
	<inherits name="gwt.material.design.themes.GwtMaterialThemeRed" />
	<inherits name="gwt.material.design.addins.GwtMaterialAddins" />
	<inherits name="org.gwtbootstrap3.extras.summernote.Summernote"/>
	<inherits name="goog.appengine.channel.Channel" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="net.videmantay.SchoolikiShared" />
	<source path="roster" />
	<source path="student" />

    <entry-point class='net.videmantay.roster.Teacher' />
 
	<replace-with class="net.videmantay.roster.ClientFactoryImpl">
		<when-type-is class="net.videmantay.roster.ClientFactory" />
	</replace-with>

	
	<set-property name="user.agent" value="safari" />
</module>