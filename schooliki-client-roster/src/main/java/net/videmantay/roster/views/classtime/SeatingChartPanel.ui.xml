<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">
	<g:HTMLPanel ui:field="seatingChart" styleName="seatingChart">
		<div class="furniturePanel">
		  <div>
		    <div style="background:white;"> <span class="seatingChartSectionTitle">Furniture</span></div>
		    <g:HTMLPanel ui:field="furniturePanel"></g:HTMLPanel>
		   </div>
		   <div>
		   <div style="background:white;"> <span class="seatingChartSectionTitle">Actions</span></div>
		    <g:HTMLPanel ui:field="actionsPanel">
		      <m:MaterialButton ui:field="rotateButton" addStyleNames="floorPlanActionButton" type="FLOATING" waves="LIGHT" size="LARGE" backgroundColor="GREEN" text="Rotate" iconType="REPLAY" />
              <m:MaterialButton ui:field="removeButton" addStyleNames="floorPlanActionButton" type="FLOATING" waves="LIGHT" size="LARGE" text="Remove" iconType="REMOVE_CIRCLE" />
		    </g:HTMLPanel>
		   </div>
		</div>
		<div ui:field="floorPlan" class="floorPlan">
		</div>
		<g:HTMLPanel ui:field="studentsPanel" addStyleNames="studentsPanel">
		       <div style="background:white;"><span class="seatingChartSectionTitle">Students</span></div>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 