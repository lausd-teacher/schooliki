<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
		<m:MaterialModal ui:field="modal" type="FIXED_FOOTER">
			<m:MaterialModalContent>
			<m:MaterialRow>
			<m:MaterialColumn>
				<m:MaterialImage ui:field="image" />
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTitle ui:field="studentsTitle"></m:MaterialTitle>
			</m:MaterialColumn>
			</m:MaterialRow>
				<m:html.Hr/>
				<m:MaterialRow>
				<m:MaterialTab>
					<m:MaterialTabItem ><m:MaterialAnchorButton text="Superb Decision" href="#posIncidentGrid"/></m:MaterialTabItem>
					<m:MaterialTabItem><m:MaterialAnchorButton text="Poor Judgement" href="#negIncidentGrid"/></m:MaterialTabItem>
				</m:MaterialTab>
				</m:MaterialRow>
				<m:MaterialRow ui:field="incidentGirdPanel">
					<m:MaterialPanel ui:field="posIncidentGrid" addStyleNames="posIncidentGrid" m:id="posIncidentGrid">
						<m:MaterialRow ui:field="posIncidentRow">
						</m:MaterialRow>
					</m:MaterialPanel>
					<m:MaterialPanel ui:field="negIncidentGrid" addStyleNames="negIncidentGrid" m:id="negIncidentGrid">
						<m:MaterialRow ui:field="negIncidentRow">
						</m:MaterialRow>
					</m:MaterialPanel>
				</m:MaterialRow>
			</m:MaterialModalContent>
			<m:MaterialModalFooter ui:field="footer">
				<m:MaterialRow marginBottom="0"> 
					<m:MaterialColumn grid="s2 m2 l2">
					<!-- Quick Assignment (All)-->
						<m:MaterialAnchorButton type="FLAT" textColor="orange darken-4" iconType="ASSIGNMENT"  ui:field="assignmentQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s2 m2 l2">
					<!-- Quick Goal (All)-->
						<m:MaterialAnchorButton type="FLAT" textColor="amber darken-4" iconType="MOOD"  ui:field="goalQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s2 m2 l2">
					<!-- Quick Form  (single/multi??)-->
						<m:MaterialAnchorButton type="FLAT" textColor="deep-orange darken-4" iconType="INBOX" ui:field="formQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s2 m2 l2">
					<!-- Quick Book  (single/multi???)-->
						<m:MaterialAnchorButton type="FLAT" textColor="red darken-4" iconType="LOCAL_LIBRARY" ui:field="bookQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s2 m2 l2">
					<!-- Show More (This only for single) -->
						<m:MaterialAnchorButton type="FLAT" textColor="red darken-2" iconType="MORE_VERT" ui:field="showMoreQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
				</m:MaterialRow>
			</m:MaterialModalFooter>
		</m:MaterialModal>
</ui:UiBinder> 