<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<m:MaterialModal ui:field="modal">
			<m:MaterialModalContent>
			<m:MaterialRow>
			<m:MaterialColumn>
				<m:MaterialImage ui:field="image" />
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTitle ui:field="studentsTitle"></m:MaterialTitle>
			</m:MaterialColumn>
			</m:MaterialRow>
				<m:html.Hr/>
				<m:MaterialRow>
				<m:MaterialTab>
					<m:MaterialTabItem href=".posIncidentGrid"><m:MaterialAnchorButton text="Superb Decision"/></m:MaterialTabItem>
					<m:MaterialTabItem href=".negIncidentGrid"><m:MaterialAnchorButton text="Poor Judgement" /></m:MaterialTabItem>
				</m:MaterialTab>
				</m:MaterialRow>
				<m:MaterialRow ui:field="incidentGirdPanel">
					<m:MaterialPanel ui:field="posIncidentGrid" addStyleNames="posIncidentGrid">
						<m:MaterialRow>
							<m:MaterialColumn ui:field="posIncidentCol" grid="s6 m4 l2"></m:MaterialColumn>
						</m:MaterialRow>
					</m:MaterialPanel>
					<m:MaterialPanel ui:field="negIncidentGrid" addStyleNames="negIncidentGrid">
						<m:MaterialRow>
							<m:MaterialColumn grid="s6 m4 l2"></m:MaterialColumn>
						</m:MaterialRow>
					</m:MaterialPanel>
				</m:MaterialRow>
			</m:MaterialModalContent>
			<m:MaterialModalFooter ui:field="footer">
				<m:MaterialRow> 
					<m:MaterialColumn>
					<!-- Quick Assignment (All)-->
						<m:MaterialAnchorButton ui:field="assignmentQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn>
					<!-- Quick Goal (All)-->
						<m:MaterialAnchorButton ui:field="goalQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn>
					<!-- Quick Form  (single/multi??)-->
						<m:MaterialAnchorButton ui:field="formQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn>
					<!-- Quick Book  (single/multi???)-->
						<m:MaterialAnchorButton ui:field="bookQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
					<m:MaterialColumn>
					<!-- Show More (This only for single) -->
						<m:MaterialAnchorButton ui:field="showMoreQuickLink"></m:MaterialAnchorButton>
					</m:MaterialColumn>
				</m:MaterialRow>
			</m:MaterialModalFooter>
		</m:MaterialModal>
	</g:HTMLPanel>
</ui:UiBinder> 