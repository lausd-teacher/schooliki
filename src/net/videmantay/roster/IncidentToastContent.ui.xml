<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:a="urn:import:com.google.gwt.dom.client">
	<ui:style>
		.toastContent{
			margin-left:auto;
			margin-right:auto;
			margin-top:20%;
	}
	</ui:style>
	<g:HTMLPanel ui:field="toastContent" addStyleNames="{style.toastContent}">
	<m:MaterialRow>
		<m:MaterialColumn grid="s12 m4 l4">
		<m:MaterialImage ui:field="studentImage" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s12 m4 l4">
			<m:MaterialLabel ui:field="summary" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s12 m4 l4">
		<g:HTMLPanel ui:field="incidentImage" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<audio ui:field="audio" />
	</g:HTMLPanel>
</ui:UiBinder> 