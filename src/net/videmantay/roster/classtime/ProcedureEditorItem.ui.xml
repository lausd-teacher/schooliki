<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:x="urn:import:gwt.material.design.addins.client.richeditor">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	<m:MaterialCard>
		<m:MaterialCardContent>
			<m:MaterialCardTitle></m:MaterialCardTitle>
			<g:HTMLPanel ui:field="htmlContent"></g:HTMLPanel> 
			<x:MaterialRichEditor visible="false" ui:field="editor" />
		</m:MaterialCardContent>
		<m:MaterialCardAction>
			<m:MaterialRow>
				<m:MaterialColumn>
					<m:MaterialAnchorButton float="LEFT" type="FLAT" iconType="EDIT"  ui:field="updateBtn"/>
					<m:MaterialAnchorButton float="RIGHT" ui:field="acceptBtn" iconType="ACCEPT" type="RAISED" />
					<m:MaterialAnchorButton float="RIGHT" ui:field="cancelBtn" iconType="CANCEL" type="RAISED" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialCardAction>
	</m:MaterialCard>
	</g:HTMLPanel>
</ui:UiBinder> 