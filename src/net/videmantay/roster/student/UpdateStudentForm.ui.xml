<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="gwt.material.design.client.ui">
		<m:MaterialPanel>
			<g:FormPanel><!--  -->
				<g:HTMLPanel>
						<m:MaterialRow>
						<m:MaterialColumn>
						<m:MaterialLabel text="Update Student" />
						</m:MaterialColumn>
						<m:MaterialColumn>
						<m:MaterialIcon ui:field="studentEditIcon" iconType="EDIT"/>
						</m:MaterialColumn>
						</m:MaterialRow>
						<m:MaterialRow>
						<table id="profileTable">
							<tbody>
								<tr><th><m:MaterialImage ui:field="stuImage" /></th></tr>
								<tr><td><div ui:field="pickerHere"><m:MaterialButton /></div></td></tr>
								<tr><td><div>
										<m:MaterialTextBox ui:field="firstName" /><br/>
										<m:MaterialTextBox ui:field="lastName"/><br/>
										<m:MaterialTextBox ui:field="extName"/><br/>
										<m:MaterialDatePicker ui:field="DOB" /><br/>
								</div></td></tr>
							</tbody>
						</table>
						</m:MaterialRow>
						<m:MaterialRow>
						<table> 
							<tbody> 
								<tr></tr>
							</tbody>
						</table>
						</m:MaterialRow>
			<div>
				<m:MaterialButton text="Cancel" ui:field="cancelBtn" /> 
				<m:MaterialButton text="Update" ui:field="updateBtn" />
			</div>	
				</g:HTMLPanel>
			</g:FormPanel>
		</m:MaterialPanel>	
</ui:UiBinder> 