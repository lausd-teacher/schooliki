<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:v="urn:import:net.videmantay.roster.views">
	<g:HTMLPanel>
		<v:RosterGrid ui:field="rosterGrid" />
		<v:RosterForm ui:field="rosterForm"/>
	<!-- FAB -->
		<m:MaterialFAB>
				<m:MaterialButton ui:field="fab" type="FLOATING"
					backgroundColor="amber" iconType="ADD" size="LARGE" />
		</m:MaterialFAB>
       	<m:MaterialModal ui:field="deleteRosterModal">
       		<m:MaterialModalContent>
       			<m:MaterialTitle title="Delete Roster" />
       			<m:MaterialLabel ui:field="deleteRosterLabel" />
       			<m:MaterialFooter>
       				<m:MaterialAnchorButton backgroundColor="grey darken-1" textColor="white"  text="Cancel" 
       				ui:field="cancelBtnDeleteRoster"/>
       				<m:MaterialAnchorButton backgroundColor="red darken-1" textColor="white"  text="OK" 
       				ui:field="okBtnDeleteRoster"/>
       			</m:MaterialFooter>
       		</m:MaterialModalContent>
       	</m:MaterialModal> 
	</g:HTMLPanel>
</ui:UiBinder> 