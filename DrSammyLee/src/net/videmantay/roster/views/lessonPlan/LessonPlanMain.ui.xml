<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
			<m:MaterialLabel text="Lesson plans" />
			<h:Hr/>
				<g:Frame url="../cal.html" ui:field="lessonPlanCal" width="100%" height="650px">
				</g:Frame>
				<m:MaterialPanel ui:field="emptyCal" visible="false">
					<m:MaterialTitle title="No Calendar Set Yet!"></m:MaterialTitle>
					<g:HTMLPanel>
						Your lesson plan is managed by Google Calendar. You can either choose one 
						from the list below or create a new one! 
						<m:MaterialRow ui:field="calendarList" />
					</g:HTMLPanel>
				</m:MaterialPanel>
				<m:MaterialFAB>
					<m:MaterialAnchorButton   size="LARGE" type="FLOATING" backgroundColor="amber darken-1" iconType="LIBRARY_BOOKS"></m:MaterialAnchorButton>
				</m:MaterialFAB>
	</g:HTMLPanel>
</ui:UiBinder> 