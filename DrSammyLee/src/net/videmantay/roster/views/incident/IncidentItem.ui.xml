<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">

	<m:MaterialPanel width=" 200xp" height="250px"
		textAlign="CENTER" backgroundColor="grey lighten-3" margin="5"
		padding="5">
		<g:HTMLPanel>
			<div ui:field="svgImage" />
			<m:MaterialLabel ui:field="name" marginBottom="5" />
			<m:MaterialChip ui:field="value" marginBottom="5"
				textColor="white" fontSize="2em" />
		</g:HTMLPanel>
	</m:MaterialPanel>
</ui:UiBinder> 