<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
		.spacer{
		height:1em;
		}
	</ui:style>
	<g:HTMLPanel ui:field="timeSlotChart">
		<m:MaterialLabel text="Rotation Chart"></m:MaterialLabel>
		<m:MaterialRow>
			<m:MaterialColumn> 
				<m:html.Span addStyleNames="{style.spacer}" />
				<m:MaterialCollection ui:field="stationCollection" />
			</m:MaterialColumn>
			<m:MaterialColumn>
		<g:FlexTable ui:field="slotsTable" />
		</m:MaterialColumn>
		</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 