<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">
	<ui:style>
		.scStudentTools{
		overflow-x:hidden;
		height:100%;
		width: 9em;
		position: relative;
		
	}
	.scStudentList{
		overflow-y:scroll;
		height:75%;
	}
	.seatingChart{
		width:1090px;
		height:650px;
		display:block;
		position:relative;
		margin-left:auto;
		margin-right:auto;
		margin-bottom:1em;
	}
	.floorPlan{
		height:650px;
		background-image:url('../img/classFloor.svg');
		border-radius:5%;
		border:0.1em dotted Brown;
		position:relative;
		left:3.75em;
	}
	</ui:style>
	<g:HTMLPanel ui:field="seatingChart" styleName="{style.seatingChart}">
	<m:MaterialRow>
	<m:MaterialColumn grid="l1">
		<h:Div addStyleNames="{style.scStudentTools}">
		<g:HTMLPanel>
			<div class="red-text text-darken-2"><span sytle="font-size:1.5em;left:0%; position:relative">Students</span>
			<span class="material-icons" style="position:relative;top:10px;left:40px">add</span></div>
		</g:HTMLPanel>
		<m:MaterialCollection  ui:field="studentList"  addStyleNames="scStudentList" />
		</h:Div>
	</m:MaterialColumn>
	<m:MaterialColumn grid="l10">
		<h:Div ui:field="floorPlan" addStyleNames="{style.floorPlan}"></h:Div>
	</m:MaterialColumn>
	</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 