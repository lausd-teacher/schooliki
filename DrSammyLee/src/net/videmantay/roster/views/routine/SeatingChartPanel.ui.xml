<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">
	<ui:style>
		.scStudentTools{
		margin:0.5em;
		padding-right:1em;
		overflow-x:hidden;
		float:left;
		height:100%;
	}
	</ui:style>
	<g:HTMLPanel ui:field="seatingChart" styleName="seatingChart">
		<h:Div addStyleNames="{style.scStudentTools}">
		<m:MaterialLabel text=" Students - No seat Assigned" />
		<m:MaterialCollection  ui:field="studentList"  addStyleNames="scStudentList" />
		<m:MaterialAnchorButton type="FLOATING" iconSize="SMALL" iconType='ADD' backgroundColor="amber darken-1" textColor="white" />
		</h:Div>
		<h:Div ui:field="floorPlan" addStyleNames="floorPlan"></h:Div>
	</g:HTMLPanel>
</ui:UiBinder> 