<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	<m:MaterialRow ui:field="seatingChartToolbar" visible="false" backgroundColor="red darken-2"> 
				<m:MaterialColumn grid="s3" > 
					<m:MaterialListBox >
						<m:html.Option text="Furniture" ><m:MaterialIcon iconType="EVENT_SEAT"/></m:html.Option>
						<m:html.Option text="Students" ><m:MaterialIcon iconType="FACE"/></m:html.Option>
						<m:html.Option text="Stations" ><m:MaterialIcon iconType="WIDGETS"/></m:html.Option>
					</m:MaterialListBox>
				</m:MaterialColumn>
				<m:MaterialColumn> 
					<m:MaterialRow ui:field="studentToolbar" visible="false"></m:MaterialRow>
					<m:MaterialRow ui:field="furnitureToolbar" visible="false"></m:MaterialRow>
					<m:MaterialRow ui:field="stationToolbar" visible="false"></m:MaterialRow>
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn  grid="m2" ui:field="scToolSideNav" >
					<m:MaterialCollapsible> 
						<m:MaterialCollapsibleItem ui:field="studentCollapseItem">
							<m:MaterialCollapsibleHeader></m:MaterialCollapsibleHeader>
							<m:MaterialCollapsibleBody>
								<m:MaterialCollection ui:field="studentList"></m:MaterialCollection>
							</m:MaterialCollapsibleBody>
						</m:MaterialCollapsibleItem>
						<m:MaterialCollapsibleItem ui:field="furnitureCollapseItem">
							<m:MaterialCollapsibleHeader></m:MaterialCollapsibleHeader>
							<m:MaterialCollapsibleBody>
								<m:MaterialCollection></m:MaterialCollection>
							</m:MaterialCollapsibleBody>
						</m:MaterialCollapsibleItem>
						<m:MaterialCollapsibleItem ui:field="stationCollapseItem">
							<m:MaterialCollapsibleHeader></m:MaterialCollapsibleHeader>
							<m:MaterialCollapsibleBody>
								<g:HTMLPanel> 
									
								</g:HTMLPanel>
								<m:MaterialCollection></m:MaterialCollection>
							</m:MaterialCollapsibleBody>
						</m:MaterialCollapsibleItem>
					</m:MaterialCollapsible>
				</m:MaterialColumn>
				<m:MaterialColumn grid="m10" >
					<m:MaterialPanel ui:field="scEditFloorPlan" />
				</m:MaterialColumn>
			</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 