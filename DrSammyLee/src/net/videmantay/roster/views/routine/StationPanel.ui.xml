<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<div class="editStationPanel">
			<div ui:field="colorIcon" />
			<m:MaterialTextBox ui:field="title" />
			<m:MaterialTextArea ui:field="description" />
			<ma:autocomplete.MaterialAutoComplete
				placeholder="group name" ui:field="group" />
		</div>
		<div class="stationPanelActive">
			<m:MaterialButton ui:field="stTitleBtn" iconType="INFO"
				iconPosition="RIGHT" />
			<m:MaterialDropDown ui:field="stDescript" />
			<m:MaterialPanel ui:field="stGroupPanel" />

		</div>
	</g:HTMLPanel>
</ui:UiBinder> 